{
    "annotations": {
        "list": []
    },
    "editable": true,
    "fiscalYearStartMonth": 0,
    "graphTooltip": 0,
    "id": null,
    "links": [],
    "liveNow": false,
    "panels": [
        {
            "title": "Scraper Success Rate",
            "type": "gauge",
            "gridPos": {
                "h": 8,
                "w": 6,
                "x": 0,
                "y": 0
            },
            "targets": [
                {
                    "expr": "avg(scraper_success_rate)",
                    "legendFormat": "Success Rate"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "percentunit",
                    "min": 0,
                    "max": 1,
                    "thresholds": {
                        "mode": "absolute",
                        "steps": [
                            {
                                "color": "red",
                                "value": null
                            },
                            {
                                "color": "yellow",
                                "value": 0.9
                            },
                            {
                                "color": "green",
                                "value": 0.95
                            }
                        ]
                    }
                }
            }
        },
        {
            "title": "Rate Limit Hits",
            "type": "timeseries",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 6,
                "y": 0
            },
            "targets": [
                {
                    "expr": "sum(rate(scraper_rate_limit_hits_total[5m])) by (endpoint)",
                    "legendFormat": "{{endpoint}}"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "reqps"
                }
            }
        },
        {
            "title": "Circuit Breaker State",
            "type": "stat",
            "gridPos": {
                "h": 8,
                "w": 6,
                "x": 18,
                "y": 0
            },
            "targets": [
                {
                    "expr": "max(scraper_circuit_breaker_state) by (endpoint)",
                    "legendFormat": "{{endpoint}}"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "mappings": [
                        {
                            "options": {
                                "0": {
                                    "text": "CLOSED",
                                    "color": "green"
                                }
                            },
                            "type": "value"
                        },
                        {
                            "options": {
                                "1": {
                                    "text": "HALF-OPEN",
                                    "color": "yellow"
                                }
                            },
                            "type": "value"
                        },
                        {
                            "options": {
                                "2": {
                                    "text": "OPEN",
                                    "color": "red"
                                }
                            },
                            "type": "value"
                        }
                    ]
                }
            }
        },
        {
            "title": "Successful Predictions",
            "type": "stat",
            "gridPos": {
                "h": 6,
                "w": 6,
                "x": 0,
                "y": 8
            },
            "targets": [
                {
                    "expr": "sum(successful_predictions_total)",
                    "legendFormat": "Total"
                }
            ]
        },
        {
            "title": "Failed Predictions",
            "type": "stat",
            "gridPos": {
                "h": 6,
                "w": 6,
                "x": 6,
                "y": 8
            },
            "targets": [
                {
                    "expr": "sum(failed_predictions_total)",
                    "legendFormat": "Total"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "thresholds": {
                        "steps": [
                            {
                                "color": "green",
                                "value": null
                            },
                            {
                                "color": "yellow",
                                "value": 10
                            },
                            {
                                "color": "red",
                                "value": 50
                            }
                        ]
                    }
                }
            }
        },
        {
            "title": "Prediction Latency",
            "type": "histogram",
            "gridPos": {
                "h": 6,
                "w": 12,
                "x": 12,
                "y": 8
            },
            "targets": [
                {
                    "expr": "histogram_quantile(0.95, sum(rate(prediction_latency_seconds_bucket[5m])) by (le))",
                    "legendFormat": "p95"
                },
                {
                    "expr": "histogram_quantile(0.50, sum(rate(prediction_latency_seconds_bucket[5m])) by (le))",
                    "legendFormat": "p50"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "s"
                }
            }
        },
        {
            "title": "Pipeline Duration by Stage",
            "type": "timeseries",
            "gridPos": {
                "h": 8,
                "w": 24,
                "x": 0,
                "y": 14
            },
            "targets": [
                {
                    "expr": "histogram_quantile(0.95, sum(rate(pipeline_duration_seconds_bucket[5m])) by (le, pipeline_stage))",
                    "legendFormat": "{{pipeline_stage}} p95"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "s"
                }
            }
        }
    ],
    "refresh": "30s",
    "schemaVersion": 38,
    "style": "dark",
    "tags": [
        "tennis",
        "scraper",
        "monitoring"
    ],
    "templating": {
        "list": []
    },
    "time": {
        "from": "now-1h",
        "to": "now"
    },
    "title": "Tennis ML - Scraper Health",
    "uid": "tennis-scraper-health",
    "version": 1
}