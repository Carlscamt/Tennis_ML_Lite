        {
            "title": "Predictions by Model Version",
            "type": "timeseries",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 8
            },
            "targets": [
                {
                    "expr": "sum(rate(model_predictions_total[1h])) by (model_version)",
                    "legendFormat": "{{model_version}}"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "reqps"
                }
            }
        },
        {
            "title": "Bets by Outcome",
            "type": "piechart",
            "gridPos": {
                "h": 8,
                "w": 6,
                "x": 12,
                "y": 8
            },
            "targets": [
                {
                    "expr": "sum(bets_placed_total) by (actual_outcome)",
                    "legendFormat": "{{actual_outcome}}"
                }
            ]
        },
        {
            "title": "Bets by Surface",
            "type": "piechart",
            "gridPos": {
                "h": 8,
                "w": 6,
                "x": 18,
                "y": 8
            },
            "targets": [
                {
                    "expr": "sum(bets_by_surface_total) by (surface)",
                    "legendFormat": "{{surface}}"
                }
            ]
        },
        {
            "title": "Feature Mean (Drift Detection)",
            "type": "timeseries",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 16
