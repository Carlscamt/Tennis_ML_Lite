            "targets": [
                {
                    "expr": "feature_mean",
                    "legendFormat": "{{feature_name}}"
                }
            ]
        },
        {
            "title": "Feature Null Rate",
            "type": "timeseries",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 12,
                "y": 16
            },
            "targets": [
                {
                    "expr": "feature_null_rate",
                    "legendFormat": "{{feature_name}}"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "percentunit",
                    "thresholds": {
                        "steps": [
                            {
                                "color": "green",
                                "value": null
                            },
                            {
                                "color": "yellow",
                                "value": 0.1
                            },
                            {
                                "color": "red",
                                "value": 0.3
                            }
                        ]
                    }
                }
            }
        }
